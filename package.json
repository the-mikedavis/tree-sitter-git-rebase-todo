{
  "name": "tree-sitter-rebase",
  "version": "0.0.1",
  "description": "Tree-sitter grammar for git's interactive rebase TODO file",
  "main": "bindings/node",
  "types": "bindings/node",
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-mikedavis/tree-sitter-rebase.git"
  },
  "keywords": [
    "parser",
    "lexer",
    "git",
    "rebase",
    "tree-sitter"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "author": "the-mikedavis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/the-mikedavis/tree-sitter-rebase/issues"
  },
  "homepage": "https://github.com/the-mikedavis/tree-sitter-rebase#readme",
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-gyp-build": "^4.8.1"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "tree-sitter-cli": "^0.22.6",
    "prebuildify": "^6.0.1"
  },
  "tree-sitter": [
    {
      "file-types": [
        "git-rebase-todo"
      ]
    }
  ]
}
